mixin nav(value = false)
    +b.NAV.nav
        +e.UL.list
            each item, index in value.navigationListItems
                - var anchor = "#" + item[1]
                +e.LI.item
                    +e.A.link(href=item[1] ? anchor : item[2] target=item[1] === false ? "_blank" : "") #{item[0]}
